{% extends 'admin/layout.njk' %}

{% block content %}

    <div class="titulo-admin">
        <p>Gerenciar receitas</p>
    </div>

    <div class="list-receitas">

        {% for item in items %}
            <div class="admin-receita">
                <img src="{{item.image}}" alt="">
                <div class="list-info">
                    <h3>{{item.title}}</h3>
                    <p>{{item.author}}</p>
                    <a href="">Visualizar</a>
                </div>
            </div>
        {% endfor %}

        

    </div>

{% endblock %}